{
    "name": "ezsystems/launchpad",
    "description": "eZ Launchpad - Ultimate eZ Platform CLI Helper.",
    "homepage": "https://github.com/ezsystems/launchpad",
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "~3.3",
        "symfony/yaml": "~3.3",
        "symfony/dependency-injection": "~3.3",
        "symfony/process": "~3.3",
        "symfony/finder": "~3.3",
        "symfony/config": "~3.3",
        "symfony/filesystem": "~3.3",
        "novactive/collection": "^1.0",
        "symfony/options-resolver": "~3.3",
        "padraic/phar-updater": "dev-use-curl",
        "symfony/event-dispatcher": "~3.3",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "symfony/var-dumper": "~3.3",
        "friendsofphp/php-cs-fixer": "^2.1",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.0",
        "behat/behat": "^3.3",
        "mikey179/vfsStream": "^1.6",
        "twig/twig": "^1.33",
        "symfony/translation": "~3.3",
        "symfony/twig-bridge": "~3.3"
    },
    "autoload": {
        "psr-4": {
            "eZ\\Launchpad\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "eZ\\Launchpad\\Tests\\": "tests/Tests"
        }
    },
    "bin": [
        "bin/ez"
    ]
}
